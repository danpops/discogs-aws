    "husky": "8.0.1",
    "jest": "28.1.1",
    "jest-html-reporter": "^3.5.0",
    "jest-tap-reporter": "^1.9.0",
    "serverless": "3.19.0",
    "serverless-offline": "8.8.0",
    "serverless-webpack": "5.7.1",
    "snazzy": "9.0.0",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.1",
    "ts-standard": "11.0.0",
